<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RedlineApp.View.MainPage"
            xmlns:local="clr-namespace:RedlineApp.View"
            BarBackgroundColor="{StaticResource AccentColor}"
            BarTextColor="{StaticResource WhiteColor}"
            SelectedTabColor="{StaticResource LightBlueColor}">
    <local:AlertPage Title="Alert" IconImageSource="hospital.png"/>
    <local:MapPage Title="Map" IconImageSource="marker.png"/>
    <local:MedicalInformationPage Title="Medical" IconImageSource="medical.png"/>
    <local:ManageAccountPage Title="Account" IconImageSource="settings.png"/>
    <local:LogoutPage Title="Logout" IconImageSource="exit.png"/>
</TabbedPage>